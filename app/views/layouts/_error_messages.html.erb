<div class="row">
  <div class="col-xs-12">
    <% if model.errors.any? %>
      <div class="alert alert-warning">
        <ul>
          <% model.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
  </div>
</div>

